#include<iostream>
using namespace std;
#include<bits/stdc++.h>
int main()
{
	long int t;cin>>t;
	
	while(t--)
	{
		long long int n,c,sum=0,r=0;
		cin>>n>>c; 
		
		for(long long int i=1;i<=n;i++)
		{ cin>>r;
		sum=sum+r;
		}
		
		if(sum<=c)
		cout<<"Yes"<<endl;
		else
		cout<<"No"<<endl;
		
		
	}
	
	return 0;
}
    
